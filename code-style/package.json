{
  "name": "@dudeofawesome/code-style",
  "version": "2.0.0-7",
  "description": "My code style configs",
  "keywords": [
    "style",
    "lint",
    "config",
    "format"
  ],
  "homepage": "https://github.com/dudeofawesome/code-style/blob/main/code-style/README.md",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dudeofawesome/code-style.git",
    "directory": "/code-style"
  },
  "license": "MIT",
  "author": "Louis Orleans <louis@orleans.io>",
  "exports": {
    "./.prettierrc": {
      "types": "./.prettierrc.d.mts",
      "default": "./.prettierrc.mjs"
    }
  },
  "bin": {
    "add-code-style": "scripts/add-code-style.sh"
  },
  "files": [
    ".prettierrc.mjs",
    ".prettierrc.mjs.map",
    ".prettierrc.d.mts",
    ".prettierrc.d.mts.map",
    "types/prettier__plugin-ruby.d.ts",
    "types/prettier-plugin-packagejson.d.ts",
    ".editorconfig",
    "scripts/add-code-style.sh"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext ts,js,json",
    "prepublishOnly": "npm run build",
    "test": "echo 'No tests'; exit 0"
  },
  "dependencies": {
    "@prettier/plugin-ruby": "^4.0.2",
    "deepmerge": "^4.3.1",
    "prettier": "^3.1.0",
    "prettier-plugin-packagejson": "^2.4.6"
  },
  "devDependencies": {
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
