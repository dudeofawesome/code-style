{
  "name": "@code-style/jest-configs",
  "version": "2.0.0-30",
  "description": "My Jest configs",
  "keywords": [
    "jest",
    "typescript"
  ],
  "homepage": "https://github.com/dudeofawesome/code-style/blob/main/packages/jest-configs/README.md",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dudeofawesome/code-style.git",
    "directory": "/packages/jest-configs"
  },
  "license": "MIT",
  "author": "Louis Orleans <louis@orleans.io>",
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/ts-cjs.js",
      "types": "./dist/ts-cjs.d.ts"
    },
    "./ts/*": {
      "default": "./dist/ts/*.js",
      "types": "./dist/ts/*.d.ts"
    },
    "./js": {
      "default": "./dist/js.js",
      "types": "./dist/js.d.ts"
    },
    "./layers/*": {
      "default": "./dist/layers/*.js",
      "types": "./dist/layers/*.d.ts"
    }
  },
  "main": "dist/ts-cjs.js",
  "files": [
    "./dist/"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext ts,js,json",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@swc/jest": "^0.2.36",
    "esbuild-jest": "^0.5.0",
    "jest-chain": "^1.1.6",
    "jest-expect-message": "^1.1.3",
    "jest-extended": "^4.0.2",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.1.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.6.3",
    "esbuild": "^0.21.0",
    "tsm": "^2.3.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@swc/core": "^1.0.0",
    "esbuild": ">=0.20.0",
    "jest": ">=29",
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "@swc/core": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
