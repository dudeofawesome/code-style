{
  "name": "@dudeofawesome/eslint-config-react",
  "version": "2.0.0-16",
  "description": "My react ESLint config",
  "keywords": [
    "style",
    "lint",
    "config",
    "eslint"
  ],
  "homepage": "https://github.com/dudeofawesome/code-style/blob/main/eslint-config-react/README.md",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dudeofawesome/code-style.git",
    "directory": "/eslint-config-react"
  },
