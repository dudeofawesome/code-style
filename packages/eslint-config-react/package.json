  "devDependencies": {
    "@types/react": "^18.2.23",
    "react": "^18.2.0",
    "tsm": "^2.3.0"
  },
  "peerDependencies": {
    "@dudeofawesome/eslint-npm-hoist-packages-react": "2.0.0-16",
    "eslint": "^8.49.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
