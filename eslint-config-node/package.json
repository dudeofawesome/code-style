{
  "name": "@dudeofawesome/eslint-config-node",
  "version": "2.0.0-7",
  "description": "My node ESLint config",
  "keywords": [
    "style",
    "lint",
    "config",
    "eslint"
  ],
  "homepage": "https://github.com/dudeofawesome/code-style/blob/main/eslint-config-node/README.md",
  "repository": {
    "type": "git",
