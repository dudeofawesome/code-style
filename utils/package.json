{
  "name": "@code-style/utils",
  "version": "2.0.0-27",
  "private": true,
  "scripts": {
    "test": "node $NODE_OPTS --require tsm --test $(glob --ignore '**/node_modules/**' --ignore '**/dist/**' '**/*[.-_]test.?(c|m)[jt]s' '**/test?(-*).?(c|m)[jt]s' '**/test/**/*.?(c|m)[jt]s')",
    "test:debug": "NODE_OPTS='--inspect-brk' npm run test"
  },
  "dependencies": {
    "eslint": "^8.51.0",
    "stylelint": "^16.3.1"
  },
  "engines": {
    "node": ">=18"
  }
}
